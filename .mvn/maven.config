# Configuración Maven para suprimir advertencias de versiones
-Dspring-boot.version.validation.skip=true
-Dspring-boot.version.check.skip=true
-Dversions.skip=true
-Dmaven.version.update.skip=true
-Dmaven.enforcer.skip=true
-Dspring-boot.version.validation.enabled=false

# Configuración del compilador
-Dmaven.compiler.showWarnings=false
-Dmaven.compiler.showDeprecation=false

# Configuración adicional para Maven
-Dspring.boot.version.check.disable=true
-Dspring.boot.maven.verbose=false